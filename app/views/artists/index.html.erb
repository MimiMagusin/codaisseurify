<h1>Overview Artists:</h1>

<% @artists.each do |artist| %>
        <div class="row">
          <div class="col-md-4">
            <h4><%= image_tag artist.image.thumbnail, class: "small-image" %></h4>
            <h4><%= link_to artist.full_name, artist_path(artist.id), class: "links" %></h4>
          </div>
        </div>
      <%end%>
