<div class="panel panel-default">
  <div class="panel-heading"><h1>Overview Artists:</h1></div>
    <div class="panel-body">

        <% @artists.each do |artist| %>
          <div class="row">
            <div class="col-md-4">
              <%= image_tag artist.image.thumbnail, class: "small-image" %>
              <h4><%= link_to artist.full_name, artist_path(artist.id), class: "links" %></h4>
            </div>
          </div>
        <%end%>

    </div>
</div>
